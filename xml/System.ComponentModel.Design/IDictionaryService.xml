      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ComponentModel.Design.IDictionaryService">
          <source>Provides a basic, component site-specific, key-value pair dictionary through a service that a designer can use to store user-defined data.</source>
          <target state="translated">サービスを通じて、デザイナーがユーザー定義データを格納するために使用できる、基本的な、コンポーネント サイト固有の、キーと値のペアのディクショナリを提供します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.Design.IDictionaryService">
          <source>This service provides a simple interface to set, retrieve, and look up objects with associated keys.</source>
          <target state="translated">このサービスは、設定、取得、および関連付けられているキーを持つオブジェクトを参照する単純なインターフェイスを提供します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ComponentModel.Design.IDictionaryService">
          <source>The following example control and designer use the <ph id="ph1">&lt;xref:System.ComponentModel.Design.IDictionaryService&gt;</ph> to share data through its site-specific key-value pair dictionary.</source>
          <target state="translated">次の例の制御とデザイナーの使用、<ph id="ph1">&lt;xref:System.ComponentModel.Design.IDictionaryService&gt;</ph>をそのサイト固有のキー/値ペアのディクショナリを使用してデータを共有します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.GetKey(System.Object)">
          <source>The value to look up in the dictionary.</source>
          <target state="translated">ディクショナリ内で検索する値。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.GetKey(System.Object)">
          <source>Gets the key corresponding to the specified value.</source>
          <target state="translated">指定した値に対応するキーを取得します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.GetKey(System.Object)">
          <source>The associated key, or <ph id="ph1">&lt;see langword="null" /&gt;</ph> if no key exists.</source>
          <target state="translated">関連付けられているキー。キーがない場合は <ph id="ph1">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.GetValue(System.Object)">
          <source>The key to look up the value for.</source>
          <target state="translated">値を検索する対象のキー。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.GetValue(System.Object)">
          <source>Gets the value corresponding to the specified key.</source>
          <target state="translated">指定したキーに対応する値を取得します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.GetValue(System.Object)">
          <source>The associated value, or <ph id="ph1">&lt;see langword="null" /&gt;</ph> if no value exists.</source>
          <target state="translated">関連付けられている値。値がない場合は <ph id="ph1">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.SetValue(System.Object,System.Object)">
          <source>An object to use as the key to associate the value with.</source>
          <target state="translated">値に関連付けるキーとして使用するオブジェクト。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.SetValue(System.Object,System.Object)">
          <source>The value to store.</source>
          <target state="translated">格納する値。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.IDictionaryService.SetValue(System.Object,System.Object)">
          <source>Sets the specified key-value pair.</source>
          <target state="translated">指定したキーと値の組み合わせを設定します。</target>       </trans-unit>
