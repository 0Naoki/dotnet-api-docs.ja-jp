    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Management.AuthenticationLevel">
          <source>Describes the authentication level to be used to connect to WMI.</source>
          <target state="translated">WMI (Windows Management Instrumentation) に接続するために使用する認証レベルを記述します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Management.AuthenticationLevel">
          <source>This is used for the COM connection to WMI.</source>
          <target state="translated">これは WMI への COM 接続で使用します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.Call">
          <source>Call-level COM authentication.</source>
          <target state="translated">呼び出しレベルの COM 認証。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.Connect">
          <source>Connect-level COM authentication.</source>
          <target state="translated">接続レベルの COM 認証。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.Default">
          <source>The default COM authentication level.</source>
          <target state="translated">既定の COM 認証レベル。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.Default">
          <source>WMI uses the default Windows Authentication setting.</source>
          <target state="translated">WMI は、既定の Windows 認証設定を使用します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.None">
          <source>No COM authentication.</source>
          <target state="translated">COM 認証なし。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.Packet">
          <source>Packet-level COM authentication.</source>
          <target state="translated">パケット レベルの COM 認証。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.PacketIntegrity">
          <source>Packet Integrity-level COM authentication.</source>
          <target state="translated">パケット整合性レベルの COM 認証。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.PacketPrivacy">
          <source>Packet Privacy-level COM authentication.</source>
          <target state="translated">パケット プライバシ レベルの COM 認証。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Management.AuthenticationLevel.Unchanged">
          <source>Authentication level should remain as it was before.</source>
          <target state="translated">認証レベルでは、以前の設定を保持してください。</target>       </trans-unit>
