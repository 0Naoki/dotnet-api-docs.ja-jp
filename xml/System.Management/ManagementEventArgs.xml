      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Management.ManagementEventArgs">
          <source>Represents the virtual base class to hold event data for WMI events.</source>
          <target state="translated">WMI イベントのイベント データを格納する仮想基本クラスを表します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Management.ManagementEventArgs.Context">
          <source>Gets the operation context echoed back                   from the operation that triggered the event.</source>
          <target state="translated">イベントを発生させた操作からエコー バックされた操作コンテキストを取得します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Management.ManagementEventArgs.Context">
          <source>Returns an <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> value for an operation context.</source>
          <target state="translated">操作コンテキストの <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> 値を返します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Management.ManagementEventArgs.Context">
          <source>Property Value</source>
          <target state="translated">プロパティ値</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Management.ManagementEventArgs.Context">
          <source>WMI context object containing context information provided by the operation that triggered the event.</source>
          <target state="translated">イベントを発生させた操作によって提供されるコンテキスト情報を含む WMI コンテキスト オブジェクト。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Management.ManagementEventArgs.Context">
          <source>.NET Framework Security</source>
          <target state="translated">.NET Framework セキュリティ</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Management.ManagementEventArgs.Context">
          <source>Full trust for the immediate caller.</source>
          <target state="translated">直前の呼び出し元に対する完全な信頼。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Management.ManagementEventArgs.Context">
          <source>This member cannot be used by partially trusted code.</source>
          <target state="translated">このメンバーは、部分的に信頼されているコードから使用することはできません。</target>       </trans-unit>
