      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Runtime.DurableInstancing.InstanceKeyView">
          <source>Provides a view into instance key information.</source>
          <target state="translated">インスタンス キー情報に関するビューを提供します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Runtime.DurableInstancing.InstanceKeyView.InstanceKey">
          <source>Gets the value of the instance key (GUID).</source>
          <target state="translated">インスタンス キー (GUID) の値を取得します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.DurableInstancing.InstanceKeyView.InstanceKey">
          <source>The value of the instance key.</source>
          <target state="translated">インスタンス キーの値です。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.Runtime.DurableInstancing.InstanceKeyView.InstanceKeyMetadata">
          <source>Gets the metadata for the instance key as a dictionary of <bpt id="p1">**</bpt>XNames<ept id="p1">**</ept> and corresponding <bpt id="p2">**</bpt>InstanceValue<ept id="p2">**</ept> objects.</source>
          <target state="translated"><bpt id="p1">**</bpt>XNames<ept id="p1">**</ept> と、対応する <bpt id="p2">**</bpt>InstanceValue<ept id="p2">**</ept> オブジェクトのディクショナリとして、インスタンス キーのメタデータを取得します。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.DurableInstancing.InstanceKeyView.InstanceKeyMetadata">
          <source>The metadata for the instance key.</source>
          <target state="translated">インスタンス キーのメタデータ。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Runtime.DurableInstancing.InstanceKeyView.InstanceKeyMetadataConsistency">
          <source>Gets consistency of the metadata (None, In Doubt, or Partial) of the instance key.</source>
          <target state="translated">インスタンスの値のメタデータ (None、In Doubt、または Partial) の一貫性を取得します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.DurableInstancing.InstanceKeyView.InstanceKeyMetadataConsistency">
