        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.IWebProxyScript">
          <source>Provides the base interface to load and execute scripts for automatic proxy detection.</source>
          <target state="translated">プロキシを自動的に検出するスクリプトを読み込んで実行するための基本インターフェイスを提供します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Net.IWebProxyScript.Close">
          <source>Closes a script.</source>
          <target state="translated">スクリプトを閉じます。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Net.IWebProxyScript.Load(System.Uri,System.String,System.Type)">
          <source>Internal only.</source>
          <target state="translated">内部でのみ使用されます。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Net.IWebProxyScript.Load(System.Uri,System.String,System.Type)">
          <source>Internal only.</source>
