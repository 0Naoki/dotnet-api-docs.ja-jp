      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Tracking.CancelRequestedRecord">
          <source>Represents the data sent by the workflow runtime to tracking participants when <ph id="ph1">&lt;see cref="M:System.Activities.ActivityExecutionContext.CancelChild" /&gt;</ph> or <ph id="ph2">&lt;see cref="M:System.Activities.ActivityExecutionContext.CancelChildren" /&gt;</ph> is called.</source>
          <target state="translated">
          <ph id="ph1">&lt;see cref="M:System.Activities.ActivityExecutionContext.CancelChild" /&gt;</ph> または <ph id="ph2">&lt;see cref="M:System.Activities.ActivityExecutionContext.CancelChildren" /&gt;</ph> の呼び出し時に、ワークフロー ランタイムによって追跡参加要素へ送られるデータを表します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>The instance ID of the activity that generated the event.</source>
          <target state="translated">イベントを生成したアクティビティのインスタンス ID。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>A sequence that defines the order of generated tracking events.</source>
          <target state="translated">生成された追跡イベントの順序を定義するシーケンス。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>The activity issuing the cancellation request.</source>
          <target state="translated">キャンセル要求を発行するアクティビティ。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>The activity to be canceled.</source>
          <target state="translated">キャンセルされるアクティビティ。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.CancelRequestedRecord" /&gt;</ph> class using the specified instance ID, record number, activity and child activity.</source>
          <target state="translated">指定したインスタンス ID、レコード番号、アクティビティ、および子アクティビティを使用して、<ph id="ph1">&lt;see cref="T:System.Activities.Tracking.CancelRequestedRecord" /&gt;</ph> クラスの新しいインスタンスを作成します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.CancelRequestedRecord.Activity">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> of the activity requesting the cancellation.</source>
          <target state="translated">キャンセルを要求するアクティビティの <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> を取得します。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.CancelRequestedRecord.Activity">
          <source>The activity information.</source>
          <target state="translated">アクティビティ情報。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.CancelRequestedRecord.Child">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> of the activity to be canceled.</source>
          <target state="translated">キャンセルされるアクティビティの <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> を取得します。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.CancelRequestedRecord.Child">
          <source>The activity information.</source>
          <target state="translated">アクティビティ情報。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.ToString">
          <source>Returns a string that represents the current <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.CancelRequestedRecord" /&gt;</ph> object.</source>
          <target state="translated">現在の <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.CancelRequestedRecord" /&gt;</ph> オブジェクトを表す文字列を返します。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.CancelRequestedRecord.ToString">
          <source>The tracking data.</source>
          <target state="translated">追跡データ。</target>       </trans-unit>
