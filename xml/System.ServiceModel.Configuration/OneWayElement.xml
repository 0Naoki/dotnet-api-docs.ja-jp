        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.OneWayElement">
          <source>Represents a configuration element that enables packet routing.</source>
          <target state="translated">パケット ルーティングを有効にする構成要素を表します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.OneWayElement">
          <source>This class cannot be inherited.</source>
          <target state="translated">このクラスは継承できません。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.OneWayElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.OneWayElement" /&gt;</ph> class.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.OneWayElement" /&gt;</ph> クラスの新しいインスタンスを初期化します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.OneWayElement.ApplyConfiguration(System.ServiceModel.Channels.BindingElement)">
          <source>The binding configuration element to apply.</source>
          <target state="translated">適用するバインド構成要素。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.OneWayElement.ApplyConfiguration(System.ServiceModel.Channels.BindingElement)">
          <source>Applies the content of a specified binding element to this configuration element.</source>
          <target state="translated">指定されたバインド要素の内容をこの構成要素に適用します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.OneWayElement.BindingElementType">
          <source>Gets the type of this binding element.</source>
          <target state="translated">このバインド要素の型を取得します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.OneWayElement.BindingElementType">
          <source>The type of this binding element.</source>
          <target state="translated">このバインド要素の型。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.OneWayElement.ChannelPoolSettings">
          <source>Gets the channel pool settings.</source>
          <target state="translated">チャネル プール設定を取得します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.OneWayElement.ChannelPoolSettings">
          <source>A <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ChannelPoolSettingsElement" /&gt;</ph> object that contains properties of the channel pool for the current channel.</source>
          <target state="translated">現在のチャネルのチャネル プールのプロパティを格納する <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ChannelPoolSettingsElement" /&gt;</ph> オブジェクト。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.OneWayElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>The configuration element to be copied.</source>
          <target state="translated">コピーされる構成要素。</target>       </trans-unit>
