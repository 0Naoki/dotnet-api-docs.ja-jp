      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.ServiceModelExtensionElement">
          <source>An abstract base class for a configuration element that contains a dynamic collection of heterogeneous child elements.</source>
          <target state="translated">異種の子要素の動的なコレクションを格納する構成要素の抽象基本クラスです。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.ServiceModelExtensionElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ServiceModelExtensionElement" /&gt;</ph> class.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ServiceModelExtensionElement" /&gt;</ph> クラスの新しいインスタンスを初期化します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.ServiceModelExtensionElement.ConfigurationElementName">
          <source>Gets the name of this configuration element.</source>
          <target state="translated">この構成要素の名前を取得します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.ServiceModelExtensionElement.ConfigurationElementName">
          <source>The unique name of this configuration element.</source>
          <target state="translated">この構成要素の一意の名前。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.ServiceModelExtensionElement.ConfigurationElementName">
          <source>The configuration extension collection does not exist.</source>
          <target state="translated">構成の拡張コレクションが存在しません。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.ServiceModelExtensionElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>The configuration element to be copied.</source>
          <target state="translated">コピーされる構成要素。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.ServiceModelExtensionElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>Copies the content of the specified configuration element to this configuration element.</source>
          <target state="translated">指定された構成要素の内容をこの構成要素にコピーします。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.ServiceModelExtensionElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
