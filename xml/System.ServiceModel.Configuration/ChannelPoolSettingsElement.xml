      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.ChannelPoolSettingsElement">
          <source>Defines channel pool settings for a custom binding.</source>
          <target state="translated">カスタム バインディングのチャネル プール設定を定義します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.ChannelPoolSettingsElement">
          <source>This class cannot be inherited.</source>
          <target state="translated">このクラスは継承できません。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.ChannelPoolSettingsElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ChannelPoolSettingsElement" /&gt;</ph> class.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ChannelPoolSettingsElement" /&gt;</ph> クラスの新しいインスタンスを初期化します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.IdleTimeout">
          <source>Get or sets the maximum time the connection can be idle before being disconnected.</source>
          <target state="translated">接続が切断されるまでの最大アイドル時間を取得または設定します。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.IdleTimeout">
          <source>A positive <ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph> that specifies the maximum time the connection can be idle before being disconnected.</source>
          <target state="translated">接続が切断されるまでの最大アイドル時間を指定する正の <ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.IdleTimeout">
          <source>The default is 00:02:00.</source>
          <target state="translated">既定値は 00:02:00 です。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.LeaseTimeout">
          <source>Gets or sets the time after which an active connection is closed.</source>
          <target state="translated">アクティブな接続が終了されるまでの時間を取得または設定します。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.LeaseTimeout">
          <source>The default is 00:05:00.</source>
          <target state="translated">既定値は 00:05:00 です。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.LeaseTimeout">
          <source>A <ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph> that specifies the time after which an active connection is closed.</source>
          <target state="translated">アクティブな接続が終了されるまでの時間を指定する <ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.LeaseTimeout">
          <source>The default is 00:05:00.</source>
          <target state="translated">既定値は 00:05:00 です。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.MaxOutboundChannelsPerEndpoint">
          <source>Gets or sets the maximum number of channels to a remote endpoint initiated by the service.</source>
          <target state="translated">サービスが開始されるリモート エンドポイントへのチャネルの最大数を取得または設定します。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.ChannelPoolSettingsElement.MaxOutboundChannelsPerEndpoint">
          <source>The maximum number of channels to a remote endpoint initiated by the service.</source>
          <target state="translated">サービスが開始されるリモート エンドポイントへのチャネルの最大数。</target>       </trans-unit>
