    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Input.ImeConversionModeValues">
          <source>Describes a mode of input conversion to be performed by an input method.</source>
          <target state="translated">入力方法で実行される入力変換のモードについて記述します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Alphanumeric">
          <source>The input method uses alphanumeric conversion mode.</source>
          <target state="translated">入力方法は、英数字変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.CharCode">
          <source>The input method uses character code conversion mode.</source>
          <target state="translated">入力方法は、文字コード変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.DoNotCare">
          <source>The input method does not care what input conversion method is used; the actual conversion method is indeterminate.</source>
          <target state="translated">入力方法は、どの入力変換方法を使用しても問題ありません。実際の変換方法は不確定です。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Eudc">
          <source>The input method uses EUDC (end user defined character) conversion mode.</source>
          <target state="translated">入力方法は、EUDC (外字) 変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Fixed">
          <source>The input method uses fixed conversion mode.</source>
          <target state="translated">入力方法は、固定変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.FullShape">
          <source>The input method uses full-shape conversion mode.</source>
          <target state="translated">入力方法は、全角変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Katakana">
          <source>The input method uses Katakana conversion mode.</source>
          <target state="translated">入力方法は、カタカナ変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Native">
          <source>The input method uses a native character (Hiragana, Hangul, Chinese) conversion mode.</source>
          <target state="translated">入力方法は、ネイティブ文字 (ひらがな、ハングル、漢字) 変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.NoConversion">
          <source>The input method will not perform any input conversion.</source>
          <target state="translated">入力方法は、どの入力変換も実行しません。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Roman">
          <source>The input method uses Roman character conversion mode.</source>
          <target state="translated">入力方法は、ローマ字変換モードを使用します。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ImeConversionModeValues.Symbol">
          <source>The input method uses symbol conversion mode.</source>
          <target state="translated">入力方法は、記号変換モードを使用します。</target>       </trans-unit>
