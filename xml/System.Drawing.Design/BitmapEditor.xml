      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Drawing.Design.BitmapEditor">
          <source>Provides a user interface for selecting bitmap files in a property browser.</source>
          <target state="translated">プロパティ ブラウザーでビットマップ ファイルを選択するためのユーザー インターフェイスを提供します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>The <ph id="ph1">&lt;xref:System.Drawing.Design.BitmapEditor&gt;</ph> class provides a simple way to select a bitmap image for a property.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Drawing.Design.BitmapEditor&gt;</ph>クラスには、プロパティのビットマップ イメージを選択する簡単な方法が用意されています。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>Starting the editor displays an <bpt id="p1">**</bpt>Open File<ept id="p1">**</ept> dialog box that allows directory navigation and selecting a file.</source>
          <target state="translated">エディターの表示を開始、<bpt id="p1">**</bpt>ファイルを開く<ept id="p1">**</ept> ダイアログ ボックス ディレクトリのナビゲーションと、ファイルを選択することができます。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>The file list is initially filtered for bitmap (.bmp) files.</source>
          <target state="translated">ビットマップ (.bmp) ファイルに対しては、ファイルの一覧は最初にフィルター処理します。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>The following code example uses an <ph id="ph1">&lt;xref:System.ComponentModel.EditorAttribute&gt;</ph> to associate the <ph id="ph2">&lt;xref:System.Drawing.Design.BitmapEditor&gt;</ph> with a property.</source>
          <target state="translated">次のコード例では、<ph id="ph1">&lt;xref:System.ComponentModel.EditorAttribute&gt;</ph>に関連付けるには、<ph id="ph2">&lt;xref:System.Drawing.Design.BitmapEditor&gt;</ph>プロパティを設定します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>for access to unmanaged code.</source>
          <target state="translated">アンマネージ コードにアクセスします。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>Associated enumeration: <ph id="ph1">&lt;see cref="F:System.Security.Permissions.SecurityPermissionFlag.UnmanagedCode" /&gt;</ph></source>
          <target state="translated">関連する列挙。 <ph id="ph1">&lt;see cref="F:System.Security.Permissions.SecurityPermissionFlag.UnmanagedCode" /&gt;</ph></target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>for full access to system resources.</source>
          <target state="translated">システム リソースへのフル アクセスできます。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>Demand values: <ph id="ph1">&lt;see cref="F:System.Security.Permissions.SecurityAction.LinkDemand" /&gt;</ph>, <ph id="ph2">&lt;see cref="F:System.Security.Permissions.SecurityAction.InheritanceDemand" /&gt;</ph>.</source>
          <target state="translated">値を要求: <ph id="ph1">&lt;see cref="F:System.Security.Permissions.SecurityAction.LinkDemand" /&gt;</ph>、<ph id="ph2">&lt;see cref="F:System.Security.Permissions.SecurityAction.InheritanceDemand" /&gt;</ph>です。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Design.BitmapEditor">
          <source>Associated state:</source>
          <target state="translated">関連付けられた状態。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Drawing.Design.BitmapEditor" /&gt;</ph> class.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Drawing.Design.BitmapEditor" /&gt;</ph> クラスの新しいインスタンスを初期化します。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.GetExtensions">
          <source>Gets the extensions for the file list filter that the bitmap editor will initially use to filter the file list.</source>
          <target state="translated">ファイル リストにフィルターを適用するために、ビットマップ エディターが最初に使用するファイル リスト フィルターの拡張子を取得します。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.GetExtensions">
          <source>The default set of file extensions used to filter the file list.</source>
          <target state="translated">ファイル リストにフィルターを適用するために使用する既定のファイル拡張子セット。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.GetFileDialogDescription">
          <source>Gets the description for the default file list filter provided by this editor.</source>
          <target state="translated">エディターによって提供される既定のファイル リスト フィルターの説明を取得します。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.GetFileDialogDescription">
          <source>The description for the default type of files to filter the file list for.</source>
          <target state="translated">ファイル リストに適用するフィルターとして使用する既定のファイルの種類に関する説明。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Drawing.Design.BitmapEditor.GetFileDialogDescription">
          <source>The description for the default type of files to filter for appears in the file list filter drop-down box.</source>
          <target state="translated">ファイル リスト フィルターのドロップダウン ボックスで、既定ファイルの種類をフィルター処理の説明が表示されます。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.LoadFromStream(System.IO.Stream)">
          <source>The stream from which to load the image.</source>
          <target state="translated">イメージの読み取り元のストリーム。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.LoadFromStream(System.IO.Stream)">
          <source>Loads an image from the specified stream.</source>
          <target state="translated">指定したストリームから画像を読み込みます。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="M:System.Drawing.Design.BitmapEditor.LoadFromStream(System.IO.Stream)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Drawing.Image" /&gt;</ph> loaded from the stream.</source>
          <target state="translated">ストリームから読み込まれた <ph id="ph1">&lt;see cref="T:System.Drawing.Image" /&gt;</ph>。</target>       </trans-unit>
