      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection">
          <source>Represents a collection of file associations.</source>
          <target state="translated">ファイルの関連付けのコレクションを表します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.Add(Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociation)">
          <source>The file association to add.</source>
          <target state="translated">追加するファイルの関連付け。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.Add(Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociation)">
          <source>Adds the specified file association to the collection.</source>
          <target state="translated">指定したファイルの関連付けをコレクションに追加します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.Clear">
          <source>Removes all file associations from the collection.</source>
          <target state="translated">コレクションからすべてのファイルの関連付けを削除します。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.Count">
          <source>Gets the number of file associations in the collection.</source>
          <target state="translated">コレクション内のファイルの関連付けの数を取得します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.Count">
          <source>The number of file associations in the collection.</source>
          <target state="translated">コレクション内のファイルの関連付けの数。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.GetEnumerator">
          <source>Returns an enumerator that can iterate through the collection.</source>
          <target state="translated">このコレクションを反復処理できる列挙子を返します。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Deployment.ManifestUtilities.FileAssociationCollection.GetEnumerator">
